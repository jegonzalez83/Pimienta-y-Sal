<!DOCTYPE html>
<html>
  <%-include("./partials/head")%> 
  <body>
    <%-include("./partials/navbar")%> 

    <%-include("./partials/header")%> 

    <!-- Page content -->
    
    <div class="ps-content">

      <!-- Menu Section -->

      <div class="ps-row ps-padding-64" id="menu">
        <div class="ps-col l6 ps-padding-large">
          <h1 class="ps-center"><%=productoSolicitado.name%> </h1>
          <br />
          <% if (productoSolicitado.description) { %>
            <h4 class="ps-text-grey"><%=productoSolicitado.description%></h4>
          <% } else { %>
            <h4 class="ps-text-grey">Plato t√≠pico</h4>
            <% } %> 
           
          <p class="ps-text-grey">
            U$S <%=productoSolicitado.price%>
          </p>
        </div>

        <div class="ps-col l6 ps-padding-large ps-img-menu">
          <img
            src="/images/<%=productoSolicitado.image%>"
            class="ps-round ps-image ps-opacity-min"
            alt="Menu"
          />
        </div>
      </div>


      <!-- Footer -->

      <%-include("./partials/footer")%> 
    </div>
  </body>
</html>
